{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "entrypoint": "dist/main.js",
          "includeFiles": [
            "dist/**"
          ]
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" }
    ]
  }